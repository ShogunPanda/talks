{
  "name": "talks",
  "version": "1.0.0",
  "description": "Public Tech Talks",
  "homepage": "https://github.com/ShogunPanda/talks",
  "repository": "https://github.com/ShogunPanda/talks",
  "bugs": {
    "url": "https://github.com/ShogunPanda/talks/issues"
