    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "freya development",
    "build": "freya build",
    "server": "freya server",
    "pdf": "freya pdf",
    "format": "prettier -w src",
    "lint": "eslint src --ext .ts,.tsx",
    "deploy": "freya deploy",
    "postdeploy": "node --import tsx src/scripts/postdeploy.ts && cd dist/deploy && netlify deploy --site $NETLIFY_SITE_ID --prod",
    "verify": "node --import tsx src/scripts/verify.ts",
    "list": "find src/talks -type d -depth 1 -exec basename {} \\; | sort"
  },
  "dependencies": {
    "freya-slides": "^0.41.1"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^9.0.0",
    "@types/react": "^18.2.46",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
